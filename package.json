{
  "name": "fitness-admin",
  "version": "1.0.0",
  "description": "Fitness Admin",
  "main": "index.js",
  "engines": {
    "node": ">=10.8.0"
  },
  "scripts": {
    "fontmin": "fontmin src/assets/font/src/* src/assets/font/dist",
    "upload": "cross-env NODE_ENV=production node build/upload.js",
    "dev": "cross-env NODE_ENV=development MODE=dev webpack-dev-server --config build/webpack.config.js --mode development --hot --host 127.0.0.1 --open --project fitness-admin",
    "build:qa": "rm -rf dist/qa && cross-env NODE_ENV=production MODE=qa webpack --mode production --hide-modules --config build/webpack.config.js --project=app --report false",
    "build:prod": "rm -rf dist/prod && cross-env NODE_ENV=production MODE=prod webpack --mode production --hide-modules --config build/webpack.config.js --project=app --report false",
    "locale:pull": "narp pull --token $POEDITOR_TOKEN",
    "locale:push": "narp push --token $POEDITOR_TOKEN",
    "lint:script": "eslint -c .eslintrc.js 'src/**/*.ts' 'src/**/*.tsx' --cache --fix",
    "lint:style": "stylelint src/**/*.scss src/**/*.css --fix",
    "lint": "run-p lint:script lint:style"
  },
  "keywords": [
    "typescript",
    "react",
    "scss",
    "webpack",
    "h5"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{scss,css}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "author": "terenyeung@gmail.com",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 10 versions",
    "not ie <= 8"
  ],
